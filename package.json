{
    "name": "refract",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "init": "npm run prebuild",
        "prepare": "npm run prebuild",
        "copy": "node ./scripts/copy",
        "generatePackages": "node ./scripts/generatePkg",
        "prebuild": "npm run generatePackages && npm run copy",
        "build": "npm run build:react && npm run build:redux && npm run build:inferno && npm run build:preact",
        "build:react": "MAIN_LIB=react rollup -c rollup.config.js",
        "build:redux": "MAIN_LIB=redux rollup -c rollup.config.js",
        "build:preact": "MAIN_LIB=preact rollup -c rollup.config.js",
        "build:inferno": "MAIN_LIB=inferno rollup -c rollup.config.js",
        "pretest": "npm run copy",
        "test:react": "jest --config base/__tests__/react/jest.config.js",
        "test:preact": "jest --config base/__tests__/preact/jest.config.js",
        "test:inferno": "jest --config base/__tests__/inferno/jest.config.js",
        "test:redux": "jest --config base/__tests__/redux/jest.config.js",
        "test": "npm run test:react && npm run test:inferno && npm run test:preact && npm run test:redux",
        "clear:jestCache": "jest --clearCache",
        "precommit": "npx lint-staged",
        "prerelease": "npm run build",
        "release": "node ./scripts/publish"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "testing-library/jest-dom",
        "@testing-library/react": "^12.1.2",
        "@types/enzyme": "^3.10.10",
        "@types/jest": "~22.2.3",
        "@types/react": "~16.8.2",
        "@typescript-eslint/eslint-plugin": "^5.8.0",
        "@typescript-eslint/parser": "^5.8.0",
        "callbag": "~1.1.0",
        "callbag-basics": "~3.0.0",
        "callbag-drop-repeats": "~1.0.0",
        "callbag-filter": "~1.0.1",
        "callbag-from-obs": "~1.2.0",
        "callbag-map": "~1.0.1",
        "callbag-of": "~1.0.1",
        "callbag-pipe": "~1.1.1",
        "callbag-start-with": "~3.1.0",
        "callbag-to-obs": "~1.0.0",
        "checksum": "~0.1.1",
        "enzyme": "^3.11.0",
        "enzyme-adapter-inferno": "^1.3.1",
        "enzyme-adapter-preact-pure": "^3.3.0",
        "eslint": "^8.5.0",
        "execa": "~0.10.0",
        "husky": "~0.14.3",
        "inferno": "~5.4.0",
        "inferno-create-element": "~5.4.0",
        "inferno-server": "~5.4.0",
        "inquirer": "~6.1.0",
        "jest": "~27.4.5",
        "lint-staged": "^12.1.3",
        "most": "~1.9.0",
        "node-fetch": "~2.2.0",
        "npm-packlist": "~1.1.11",
        "ora": "~3.0.0",
        "p-iteration": "~1.1.7",
        "pacote": "~9.1.0",
        "preact": "~10.6.4",
        "prettier": "~2.5.1",
        "react": "~17.0.2",
        "react-dom": "~17.0.2",
        "recursive-readdir": "~2.2.2",
        "redux": "~4.1.2",
        "rollup": "~2.61.1",
        "rollup-plugin-typescript2": "~0.31.1",
        "rxjs": "~7.1.0",
        "rxjs-compat": "~6.1.0",
        "symbol-observable": "~1.2.0",
        "tar": "~4.4.6",
        "ts-jest": "~27.1.2",
        "typescript": "^4.5.4",
        "xstream": "~11.7.0"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "prettier --write",
            "eslint"
        ],
        "*.{js,md}": [
            "prettier --write"
        ]
    }
}
